<?xml version="1.0" encoding="UTF-8"?>
<testcase id="bcfoBreakAfter5" xmlns:fo="http://www.w3.org/1999/XSL/Format">
    <fo foName="block-container">
      <foProperty name="break-after" value="page"/>
      <fo foName="block">
       <foText>This test generate three pages.  This text appear on the first page</foText>
      </fo>
    </fo>
    <fo foName="block-container">
      <foProperty name="break-after" value="odd-page"/>
      <fo foName="block">
       <foText>This text appear on the second page.</foText>
      </fo>
    </fo>
    <fo foName="block-container">
      <fo foName="block">
       <foText>This text appear on the third page.</foText>
      </fo>
    </fo>
  </testcase>
