<?xml version="1.0" encoding="UTF-8"?>
<testcase id="bcfoBreakBefore3" xmlns:fo="http://www.w3.org/1999/XSL/Format">
    <fo foName="block-container">
      <fo foName="block">
       <foText>This test generate four pages (page number three should be blank). This text appear on the first page.</foText>
      </fo>
    </fo>
    <fo foName="block-container">
      <foProperty name="break-before" value="even-page"/>
      <fo foName="block">
       <foText>This text appear on the second page.</foText>
      </fo>
    </fo>
    <fo foName="block-container">
      <foProperty name="break-before" value="even-page"/>
      <fo foName="block">
       <foText>This text appear on the fourth page.</foText>
      </fo>
    </fo>
  </testcase>
